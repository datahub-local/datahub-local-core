architecture: standalone

auth:
  existingSecret: "valkey-admin-credentials"
  existingSecretPasswordKey: "password"

metrics:
  enabled: true

  extraEnvVars:
    - name: REDIS_PASSWORD
      valueFrom:
        secretKeyRef:
          name: valkey-admin-credentials
          key: password

  podMonitor:
    enabled: true

  serviceMonitor:
    enabled: true
