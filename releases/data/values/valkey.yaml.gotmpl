architecture: standalone

auth:
  existingSecret: "valkey-admin-credentials"
  existingSecretPasswordKey: "password"

metrics:
  enabled: true

  podMonitor:
    enabled: true

    extraEnvVars:
      - name: REDIS_PASSWORD
        valueFrom:
          secretKeyRef:
            name: valkey-admin-credentials
            key: password

  serviceMonitor:
    enabled: true
